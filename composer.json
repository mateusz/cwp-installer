{
    "name": "cwp/cwp-installer",
    "type": "silverstripe-recipe",
    "description": "CWP Project Template",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=7.1.0",
        "silverstripe/recipe-plugin": "^1",
        "cwp/cwp-recipe-core": "2.8.0-rc1",
        "cwp/cwp-recipe-cms": "2.8.0-rc1",
        "silverstripe/recipe-blog": "1.8.0-rc1",
        "silverstripe/recipe-form-building": "1.8.0-rc1",
        "silverstripe/recipe-authoring-tools": "1.8.0-rc1",
        "silverstripe/recipe-collaboration": "1.8.0-rc1",
        "silverstripe/recipe-reporting-tools": "1.8.0-rc1",
        "cwp/cwp-recipe-search": "2.8.0-rc1",
        "silverstripe/recipe-services": "1.8.0-rc1",
        "tractorcow/silverstripe-fluent": "4.5.1@stable",
        "silverstripe/registry": "2.3.0-rc1",
        "cwp/starter-theme": "3.2.0-rc1",
        "silverstripe/mfa": "4.3.0-rc1",
        "silverstripe/totp-authenticator": "4.1.1@stable"
    },
    "suggest": {
        "silverstripe/recipe-content-blocks": "Supplement the CMS with a 'content block' approach to editing"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3"
    },
    "extra": {
        "project-files": [
            "app/*"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}